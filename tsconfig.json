{
  "compilerOptions": {
    "target": "es6",
    "module": "esnext",
    "jsx": "react",
    "strict": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "plugins": [{ "name": "typescript-plugin-css-modules" }],
    "baseUrl": ".",
    "paths": {
      "@mui/material": ["node_modules/@mui/material"]
    }
  },
  "include": ["./src/**/*","env.d.ts"],
  "exclude": ["node_modules"]
}
